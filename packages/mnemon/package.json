{
  "name": "@elite-agents/mnemon",
  "version": "0.0.1",
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:all": "bun install && bun build ./src/index.ts --target node --outdir ./dist && bun run build:types",
    "dev": "bun run build:all --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "publish": "bun run prepare && npm publish --access=public"
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "@elite-agents/oldowan": "*",
    "@elite-agents/machina-habilis": "*",
    "ai": "latest",
    "drizzle-orm": "^0.39.3",
    "falkordb": "latest",
    "nanoid": "latest",
    "postgres": "^3.4.5",
    "zod": "latest"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
