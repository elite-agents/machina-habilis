{
  "name": "@elite-agents/mnemon",
  "version": "0.0.5",
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:node": "bun build ./src/index.ts --outdir ./dist --target node && bun run build:types",
    "dev": "bun run build -watch",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "publish:package": "bun run build:node && npm publish --access=public"
  },
  "dependencies": {
    "@elite-agents/oldowan": "*",
    "@elite-agents/machina-habilis": "*",
    "nanoid": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
