{
  "name": "@elite-agents/machina-habilis",
  "version": "0.4.0",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --external @solana/kit --external openai --external hono --external zod && bun run build:types",
    "build:all": "bun install && bun build ./src/index.ts --outdir ./dist && bun run build:types",
    "dev": "bun run build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@elite-agents/oldowan": "workspace:*",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "bundledDependencies": [
    "@elite-agents/oldowan"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@solana/kit": "^2.1.0",
    "@types/json-schema": "^7.0.15",
    "hono": "^4.7.5",
    "nanoid": "^5.0.9",
    "openai": "^4.90.0",
    "zod": "^3.24.2"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
